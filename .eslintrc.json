{
    "root": true,
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
  "rules": {
    "quotes": ["error", "single"],
    "no-trailing-spaces": "error",
    "no-empty-function": "error",
    "no-empty": "error"
  }
}